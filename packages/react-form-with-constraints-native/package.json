{
  "name": "react-form-with-constraints-native",
  "version": "0.13.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkrotoff/react-form-with-constraints.git"
  },
  "description": "Simple form validation for React",
  "license": "MIT",
  "keywords": [
    "react",
    "form",
    "validation",
    "react-form-with-constraints",
    "react-native"
  ],
  "main": "lib-es5/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/",
    "lib-es5/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf lib lib-es5 coverage .rpt2_cache",
    "tsc": "tsc",
    "build": "yarn build:esnext && yarn build:es5",
    "build:esnext": "tsc --project tsconfig.lib.json",
    "build:es5": "tsc --project tsconfig.lib-es5.json",
    "prepublishOnly": "yarn clean && yarn tsc && yarn build",
    "test": "node --trace-warnings node_modules/.bin/jest --no-cache --verbose",
    "test:coverage": "node --trace-warnings node_modules/.bin/jest --no-cache --coverage"
  },
  "peerDependencies": {
    "react-form-with-constraints": "^0.13.0",
    "react-native": ">=0.57.8"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.14",
    "@types/react": "^16.8.20",
    "@types/react-native": "^0.57.63",
    "@types/react-test-renderer": "^16.8.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "react": "^16.8.6",
    "react-form-with-constraints": "^0.13.0",
    "react-native": "^0.59.9",
    "react-test-renderer": "^16.8.6",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  }
}
